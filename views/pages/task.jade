extends ../layout
script(type="text/javascript", src='/js/utils.js')
block content
   .container
      .row
         .panel
            span.btn-lg 新建打包任务
            a.btn.btn-default.navbar-right(type='button',href='./') 取消
            a.btn.btn-default.navbar-right(type='button',href='./task') 保存
         .panel.panel-default
            .panel-heading
               span.label.label-default 选择微博版本
            .panel-body
               span.small 微博版本：
                  select#verSelect(style="width:105px; height:27px;", onclick='jumpTo()')
                     if weiboVersions
                        if weiboVersions.length > 0
                           each weiboVersion in weiboVersions
                              option  #{weiboVersion.version_name}

                        else
                           option  暂无数据

                     else
                        option  暂无数据
                     option(value='0') 创建新版本
            .panel-body
               span.small 任务描述：
               input
         .panel.panel-default
            .panel-heading
               span.label.label-default 基本配置
            .panel-body
